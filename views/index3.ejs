<!DOCTYPE html>
<html>

<head>
  <title>Tool test</title>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <link rel="stylesheet" href="./css/w3.css">
  <link rel="stylesheet" href="./css/theme.css">
  <link rel="stylesheet" href="./fontawesome/css/fontawesome.css">
  <link rel="stylesheet" href="./fontawesome/css/all.css">

  <link rel="stylesheet" href="./lib/date-time-picker/jquery-ui.css">
  <link rel="stylesheet" href="./lib/keyboard/jquery-ui.min.css">
  <link rel="stylesheet" href="./lib/keyboard/keyboard.css">

  <link rel="stylesheet" href="./css/index.css">
  
  

<%- include('./menu/ejs')%>

  <style>
  html,body,h1,h2,h3,h4,h5,h6 {font-family: "Roboto", sans-serif;}
  .w3-sidebar {
    z-index: 3;
    width: 250px;
    top: 43px;
    bottom: 0;
    height: inherit;
  }
  </style>
  

</head>

<body oncontextmenu="return false">
  <%- include ('./menu/html') %>
  <div class="w3-top">
  <div class="w3-bar w3-theme-d5 w3-top w3-center-align w3-large">
    
    <div class="w3-hide-small w3-hover-white" style="width: 300px">
      <i class="fas fa-user" style="float: left;margin: 11px;"></i>
      <input id="student_name" class="w3-input w3-border" type="text" placeholder="Student name" style="padding: 0 11px; width: 200px;float: left; margin: 6px;" onclick="keyboard('student_name')" onfocusout="$('#student_name_keyboard').hide()">
    </div>
    <div style="float: left;width: calc(100% - 300px);text-align: center;line-height: 39px;">
      Test
    </div>
    <div id="mode_chart" class="w3-bar-item w3-button w3-hide-small w3-hide-medium w3-hover-white w3-right" onclick="modeChart()"><i class="fas fa-columns fa-rotate-270"></i></div>
  </div>
</div>

<!-- Sidebar -->
<nav class="w3-sidebar w3-bar-block w3-collapse w3-large w3-theme-l1 w3-animate-left" id="mySidebar">
  <!-- <a href="javascript:void(0)" onclick="w3_close()" class="w3-right w3-xlarge w3-padding-large w3-hover-black w3-hide-large" title="Close Menu">
    <i class="fa fa-remove"></i>
  </a> -->
  
</nav>

<!-- Overlay effect when opening sidebar on small screens -->
<div class="w3-overlay w3-hide-large" onclick="w3_close()" style="cursor:pointer" title="close side menu" id="myOverlay"></div>

<!-- Main content: shift it to the right by 250 pixels when the sidebar is visible -->
<div class="w3-main w3-white" style="margin-left:250px; height: 100%; padding-top: 41px;">
  <div id="mode_chart_1">
    <div id="view_chart_0" class="chart">
      <div class="w3-border w3-round-xlarge w3-card w3-border-red">
        <div class="view_chart"></div>
        <div id="info_chart_0" class="info w3-theme-l4"></div>
      </div>
      
    </div>
  </div>
  <div id="mode_chart_2">
    <div id="view_chart_1" class="chart">
      <div class="w3-border w3-round-xlarge w3-card w3-border-red">
        <div   class="view_chart"></div>
        <div id="info_chart_1" class="info w3-theme-l4"></div>
      </div>
    </div>
    <div id="view_chart_2" class="chart">
      <div class="w3-border w3-round-xlarge w3-card w3-border-red">
        <div class="view_chart"></div>
        <div id="info_chart_2" class="info w3-theme-l4"></div>
      </div>
    </div>

  </div>


<!-- END MAIN -->
</div>





  <script src="./fontawesome/js/all.js"></script>
  <script src="./lib/jquery/jquery-3.4.1.min.js"></script>
  <script src="./lib/jquery/js.cookie-1.5.1.js"></script>
  <script src="./socket.io/socket.io.js"></script>

  <script src="./lib/slimscroll/jquery.slimscroll.js"></script>
  <script src="./js/w3.js"></script>
  <script src="./lib/chart.min.js"></script>

<script src="./lib/keyboard/jquery.keyboard.js"></script>
<script src="./lib/keyboard/jquery.keyboard.extension-extender.js"></script>
<script src="./lib/keyboard/jquery.keyboard.extension-navigation.js"></script>



  <script src="./js/config.js"></script>
  <script src="./js/common.js"></script>
  <script src="./js/keyboard.js"></script>
  <script src="./js/list.js"></script>
  <script src="./js/detail.js"></script>
  <script src="./js/index.js"></script>

</body>
</html>
